Host *
 IdentityFile ~/.ssh/id_rsa
 TCPKeepAlive=yes
 ServerAliveInterval=15
 ServerAliveCountMax=6
 StrictHostKeyChecking=no
 Compression=yes
 ForwardAgent=yes
 # Send client's locale
 SendEnv LANG LC_*
 # Re-use SSH connection
 ControlMaster auto
 ControlPath ~/.ssh/sockets/%r@%h-%p
 ControlPersist yes
 # Platform-specific options
 IgnoreUnknown AddKeysToAgent,UseKeychain 
 AddKeysToAgent yes
 UseKeychain yes
Host maxpro
  HostName maxpro.dedyn.io
  User ok
  Port 8082
Host timeweb
  HostName kant.timeweb.ru
  User mobistyle
Host hts
  HostName ssh-srv66105.ht-systems.ru
  User srv66105
Match host mediabox exec "ping -c 1 -W 5 %h > /dev/null 2>&1"
   HostName mediabox
Match host mediabox !exec "ping -c 1 -W 5 %h > /dev/null 2>&1"
   AddressFamily inet6
   HostName hirsch.dedyn.io
